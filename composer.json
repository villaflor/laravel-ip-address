{
    "name": "villaflor/laravel-ip-address",
    "description": "A Laravel library for retrieving accurate public IP address of the server.",
    "keywords": [
        "laravel",
        "ip",
        "address"
    ],
    "homepage": "https://github.com/villaflor/laravel-ip-address",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Anthony M. Villaflor",
            "email": "villaflor@live.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.12"
    },
    "autoload": {
        "psr-4": {
            "Villaflor\\IpAddress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Villaflor\\IpAddress\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Villaflor\\IpAddress\\IpAddressServiceProvider"
            ]
        }
    }
}
